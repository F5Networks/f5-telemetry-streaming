{
    "$id": "schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "",
    "description": "",
    "type": "object",
    "properties": {
        "system": {
            "type": "object",
            "properties": {
                "hostname": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
                "versionBuild": {
                    "type": "string"
                },
                "location": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "marketingName": {
                    "type": "string"
                },
                "platformId": {
                    "type": "string"
                },
                "chassisId": {
                    "type": "string"
                },
                "baseMac": {
                    "type": "string"
                },
                "callBackUrl": {
                    "type": "string"
                },
                "configReady": {
                    "type": "string"
                },
                "licenseReady": {
                    "type": "string"
                },
                "provisionReady": {
                    "type": "string"
                },
                "syncMode": {
                    "type": "string"
                },
                "syncColor": {
                    "type": "string"
                },
                "syncStatus": {
                    "type": "string"
                },
                "syncSummary": {
                    "type": "string"
                },
                "failoverStatus": {
                    "type": "string"
                },
                "failoverColor": {
                    "type": "string"
                },
                "systemTimestamp": {
                    "type": "string"
                },
                "cpu": {
                    "type": "integer"
                },
                "memory": {
                    "type": "integer"
                },
                "tmmCpu": {
                    "type": "integer"
                },
                "tmmMemory": {
                    "type": "integer"
                },
                "tmmTraffic": {
                    "type": "object"
                },
                "diskStorage": {
                    "type": "object"
                },
                "diskLatency": {
                    "type": "object"
                },
                "networkInterfaces": {
                    "type": "object"
                },
                "provisioning": {
                    "type": "object"
                },
                "asmState": {
                    "type": "string"
                },
                "lastAsmChange": {
                    "type": "string"
                },
                "apmState": {
                    "type": "string"
                },
                "afmState": {
                    "type": "string"
                },
                "lastAfmDeploy": {
                    "type": "string"
                },
                "ltmConfigTime": {
                    "type": "string"
                },
                "gtmConfigTime": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "hostname",
                "version",
                "versionBuild",
                "location",
                "description",
                "marketingName",
                "platformId",
                "chassisId",
                "baseMac",
                "callBackUrl",
                "syncMode",
                "syncColor",
                "syncStatus",
                "syncSummary",
                "failoverStatus",
                "failoverColor",
                "systemTimestamp",
                "cpu",
                "memory",
                "tmmCpu",
                "tmmMemory",
                "tmmTraffic",
                "diskStorage",
                "diskLatency",
                "networkInterfaces",
                "provisioning"
            ],
            "allOf": [
                {
                    "if": {
                        "properties": { "version": { "pattern": "^1[4-9]" } }
                    },
                    "then": {
                        "required": [
                            "configReady",
                            "licenseReady",
                            "provisionReady"
                        ]
                    }
                }
            ]
        },
        "virtualServers": {
            "type": "object"
        },
        "pools": {
            "type": "object"
        },
        "ltmPolicies": {
            "type": "object"
        },
        "httpProfiles": {
            "type": "object"
        },
        "clientSslProfiles": {
            "type": "object"
        },
        "serverSslProfiles": {
            "type": "object"
        },
        "sslCerts": {
            "type": "object"
        },
        "iRules": {
            "type": "object"
        },
        "telemetryServiceInfo": {
            "type": "object",
            "properties": {
                "pollingInterval": {
                    "type": "integer"
                },
                "cycleStart": {
                    "type": "string"
                },
                "cycleEnd": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "pollingInterval",
                "cycleStart",
                "cycleEnd"
            ]
        },
        "networkTunnels": {
            "type": "object"
        },
        "telemetryEventCategory": {
            "type": "string",
            "enum": [ "systemInfo" ]
        },
        "deviceGroups": {
            "type": "object"
        }
    },
    "additionalProperties": false,
    "required": [
        "system",
        "virtualServers",
        "pools",
        "ltmPolicies",
        "httpProfiles",
        "clientSslProfiles",
        "serverSslProfiles",
        "sslCerts",
        "telemetryServiceInfo",
        "networkTunnels",
        "telemetryEventCategory"
    ]
}
